module Stigg
  module Internal
    module Type
      module Enum
        include Stigg::Internal::Type::Converter
        include Stigg::Internal::Util::SorbetRuntimeSupport

        def self.values: -> ::Array[(nil | bool | Integer | Float | Symbol)]

        def ===: (top other) -> bool

        def ==: (top other) -> bool

        def hash: -> Integer

        def coerce: (
          String | Symbol | top value,
          state: Stigg::Internal::Type::Converter::coerce_state
        ) -> (Symbol | top)

        def dump: (
          Symbol | top value,
          state: Stigg::Internal::Type::Converter::dump_state
        ) -> (Symbol | top)

        def to_sorbet_type: -> top

        def inspect: (?depth: Integer) -> String
      end
    end
  end
end
